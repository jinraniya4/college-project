<% layout("/layouts/boilerplate") -%>



<div class="card col-6 offset-2">

    <img src="<%= listing.image %>" class="card-img-top show-img" alt="listing_image">
    <div class="card-body">
      <p class="card-text">
        <b>
          <%= listing.title %>
        </b>
        
       
          <br>
         Price - &#8377; <%= listing.price.toLocaleString("en-IN") %>
            
              <br>
            Contact -  <%= listing.country %>

      </p>
    </div>

  </div>